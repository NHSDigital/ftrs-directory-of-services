model {

  extend ftrs {

    api dos-search "DoS Search API" {
      description '
        /gp-search
        API for searching GP services
      '

      lambda get_organization "GET /Organization?ods_code={ods_code}" {
        description 'Retrieves a specific organization by ODS code'
      }
    }
  }

  apim.dosSearchProxy -> dos-search.get_organization "Proxies calls to"
  get_organization -> ftrs.db.orgTable "Reads from"
  ps1 -> apim.dosSearchProxy "GET data from"
}
