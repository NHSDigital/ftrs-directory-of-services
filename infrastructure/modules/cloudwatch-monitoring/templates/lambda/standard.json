{
  "lambda": [
    {
      "metric_name": "Duration",
      "statistic": "p99",
      "comparison_operator": "GreaterThanThreshold",
      "alarm_suffix": "duration-p99-critical",
      "description": "Lambda duration p99 critical threshold",
      "severity": "critical"
    },
    {
      "metric_name": "Errors",
      "statistic": "Sum",
      "comparison_operator": "GreaterThanThreshold",
      "alarm_suffix": "errors-critical",
      "description": "Lambda errors critical threshold",
      "severity": "critical"
    },
    {
      "metric_name": "Throttles",
      "statistic": "Sum",
      "comparison_operator": "GreaterThanThreshold",
      "alarm_suffix": "throttles-critical",
      "description": "Lambda throttles critical threshold",
      "severity": "critical"
    },
    {
      "metric_name": "ConcurrentExecutions",
      "statistic": "Maximum",
      "comparison_operator": "GreaterThanOrEqualToThreshold",
      "alarm_suffix": "concurrent-executions-critical",
      "description": "Lambda concurrent executions critical threshold",
      "severity": "critical"
    }
  ]
}
